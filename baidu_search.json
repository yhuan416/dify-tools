{
    "openapi": "3.1.0",
    "info": {
        "title": "百度搜索 API",
        "description": "百度搜索接口的 OpenAPI 配置文件",
        "version": "v1.0.0"
    },
    "servers": [
        {
            "url": "https://baidu.com"
        }
    ],
    "paths": {
        "/s": {
            "get": {
                "description": "百度搜索接口",
                "operationId": "baidu_search",
                "parameters": [
                    {
                        "name": "wd",
                        "in": "query",
                        "description": "查询关键词",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "tn",
                        "in": "query",
                        "description": "返回结果的格式（json）",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "json"
                        }
                    },
                    {
                        "name": "rn",
                        "in": "query",
                        "description": "返回的结果数量",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 10
                        }
                    },
                    {
                        "name": "ie",
                        "in": "query",
                        "description": "返回的编码格式（utf-8）",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "utf-8"
                        }
                    }
                ],
                "deprecated": false
            }
        }
    },
    "components": {
        "schemas": {}
    }
}